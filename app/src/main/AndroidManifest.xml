<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.fivefivelike.themvpbase">

    <application
        android:name=".BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvp.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".mvp.activity.NormalWebviewActivity"/>
        <activity android:name=".mvp.activity.ImageActivity"/>
        <activity android:name=".mvp.activity.NormalRequestActivity"/>
        <activity android:name=".mvp.activity.PageRequestActivity"/>
        <activity android:name=".mvp.activity.RollViewActivity"/>
        <activity android:name=".mvp.activity.HeadExpandActivity"/>
        <activity android:name=".mvp.activity.ImageChooseActivity"/>
    </application>

</manifest>